pico-8 cartridge // http://www.pico-8.com
version 19
__lua__
local r1x0 = 10
local r1y0 = 10
local r1x1 = 50
local r1y1 = 50
local r1c = 2

local r2x0 = 60
local r2y0 = 60
local r2x1 = 100
local r2y1 = 100
local r2c = 3

local r1dx = .5
local r1dy = 2

local r2dx = 2
local r2dy = 2

local sprx = 40;
local spry = 40;

local sprn = 7;

function _draw()
 cls()

 rectfill(0, 0, 127, 127, 0)
 map(0, 0, 0, 0, 3, 5)
 pset(1, 80, 10)
 pset(1.99, 80, 10)
 
 print("abcdefghijklmnopqrstuvwxyz", 0, 0, 7)
 
 camera(-64, -64)
 
 -- draw a big red circle, centered
 circfill(0, 0, 10, 8)
 
 camera(0,0)
 
 rectfill(50, -1, 60, 10, 11)

 rectfill(r1x0, r1y0, r1x1, r1y1, r1c)

 rect(r2x0, r2y0, r2x1, r2y1, r2c)

 pset(40, 10, 13)
 
 line(120, 10, 120, 120, 14)
 
 line(0, 0, 20, 20, 15)
 
 line(0, 20, 20, 0, 15)
 
 line(20, 30, 0, 20, 12)
 
 line(0, 30, 20, 20, 12)

 circ(90, 90, 30, 8)

 circfill(100, 100, 15, 9)
 
 print(fget(1), 64, 64, 7)
 print(fget(8, 3), 64, 72, 7)
 print(fget(255), 64, 80, 7)
 
 print(sget(59,5), 64, 88, 7)
 print(sget(60,6), 64, 96, 7)
 
 spr(sprn, sprx, spry, 1, 1, false, false)
 
 sspr(64, 0, 8, 8, 72, 48, 12, 20, false, false)
 
 clip(0, 8, 128, 112)
 circfill(10, 118, 8, 8)
 clip()
 circfill(40, 118, 8, 7)
end

function _update()
    if btn(0) then sprx = sprx - 2 end
	if btn(1) then sprx = sprx + 2 end
	if btn(2) then spry = spry - 2 end
	if btn(3) then spry = spry + 2 end
	if btnp(4) then
		local val = fget(8, 3)
		fset(8, 3, not val)
		sprn +=1 
		sset(59,5, 8)
	end
    if btnp(5) then 
		local val = fget(8, 3)
		fset(8, 3, not val)
		sprn -=1 
		sset(60,6, 8)
	end

    if (r1x0 < 0 or r1x1 > 127) then
        r1dx = -1 * r1dx;
    end
    if (r1y0 < 0 or r1y1 > 127) then
        r1dy = -1 * r1dy;
    end
    if (r2x0 < 0 or r2x1 > 127) then
        r2dx = -1 * r2dx;
    end
    if (r2y0 < 0 or r2y1 > 127) then
        r2dy = -1 * r2dy;
    end
    r1x0 = r1dx + r1x0
    r1x1 = r1dx + r1x1
    r1y0 = r1dy + r1y0
    r1y1 = r1dy + r1y1
end
